# Example environment configuration for 'cloud' deployment type
# Enables: git, docker, golang, helm, gcloud, ssh

# Deployment type
export DEPLOYMENT_TYPE=cloud

# Proxy settings (usually not needed in cloud environments)
export PROXY_HOST=
export PROXY_PORT=8080
export NO_PROXY=localhost,127.0.0.1,.local,metadata.google.internal

# Git configuration
export GIT_USER_NAME="Cloud Developer"
export GIT_USER_EMAIL="cloud.dev@example.com"

# Apply with: chezmoi init --apply https://github.com/hiroyoshii/dotfiles.git
